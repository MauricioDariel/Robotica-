    "%% 4. ANÁLISIS DE RESULTADOS\n",
    "P1 = abs(tau1 .* t1_d); P2 = abs(tau2 .* t2_d); P3 = abs(tau3 .* t3_d);\n",
    "P_total = P1 + P2 + P3;\n",
    "\n",
    "% --- Gráfica Resumen ---\n",
    "figure('Name', 'Resultados Dinámicos', 'Position', [100 100 1000 600]);\n",
    "\n",
    "subplot(2,2,1); plot(tsim, w, 'b', 'LineWidth', 1.5); grid on;\n",
    "title('Manipulabilidad'); xlabel('Tiempo [s]'); ylabel('w');\n",
    "\n",
    "subplot(2,2,2);\n",
    "plot(tsim, tau1, 'r', tsim, tau2, 'g', tsim, tau3, 'b'); grid on;\n",
    "title('Torque Requerido'); legend('M1','M2','M3'); ylabel('Nm');\n",
    "\n",
    "subplot(2,2,3);\n",
    "plot(tsim, P1, 'r', tsim, P2, 'g', tsim, P3, 'b'); grid on;\n",
    "title('Potencia Instantánea'); legend('P1','P2','P3'); ylabel('Watts');\n",
    "\n",
    "subplot(2,2,4);\n",
    "bar([rms(tau1), rms(tau2), rms(tau3); max(abs(tau1)), max(abs(tau2)), max(abs(tau3))]');\n",
    "legend('RMS (Nominal)', 'Pico'); title('Dimensionamiento de Motores');\n",
    "xticklabels({'Motor 1', 'Motor 2', 'Motor 3'}); ylabel('Torque [Nm]');"
   ],
