    "%% 1. PARÁMETROS Y TRAYECTORIA\n",
    "clear all; close all; clc;\n",
    "\n",
    "% --- Parámetros Geométricos y Dinámicos ---\n",
    "L1 = 0.5; L2 = 0.5; L3 = 0.25;      % [m]\n",
    "m1 = 2.0; m2 = 1.5; m3 = 0.8;       % [kg]\n",
    "I1 = 0.1; I2 = 0.08; I3 = 0.02;     % [kg·m²]\n",
    "lc1 = L1/2; lc2 = L2/2; lc3 = L3/2; % Centros de masa\n",
    "g = 9.81;\n",
    "\n",
    "% --- Generación de Trayectoria Elíptica ---\n",
    "tf = 10; N = 1001;\n",
    "tsim = linspace(0, tf, N);\n",
    "dt = tsim(2) - tsim(1);\n",
    "\n",
    "centro_x = 0.5; centro_y = 0.0;\n",
    "a = 0.4; b = 0.2;\n",
    "\n",
    "x = centro_x + a * cos(2*pi*tsim/tf);\n",
    "y = centro_y + b * sin(2*pi*tsim/tf);\n",
    "theta = atan2(y-centro_y, x-centro_x) + pi/2;\n",
    "\n",
    "% Derivadas para velocidad y aceleración (Trayectoria)\n",
    "vx = gradient(x, dt); vy = gradient(y, dt);\n",
    "ax = gradient(vx, dt); ay = gradient(vy, dt);\n",
    "\n",
    "% Visualización preliminar\n",
    "figure('Name','Trayectoria'); plot(x,y,'b-'); axis equal; grid on;\n",
    "title('Trayectoria Geométrica Propuesta'); xlabel('x [m]'); ylabel('y [m]');"
   ],
